{
  "compilerOptions": {
    "target": "es2016",
    "module": "node16",
    "lib": ["dom", "es2015"],
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "allowImportingTsExtensions": true,
    "outDir": "dist"
  },
  "include": ["src"]
}
